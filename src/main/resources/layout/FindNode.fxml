<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?><?import javafx.scene.control.Label?><?import javafx.scene.control.TextField?><?import javafx.scene.control.ToolBar?><?import javafx.scene.layout.HBox?><?import javafx.scene.layout.StackPane?><?import javafx.scene.layout.VBox?>
<VBox fx:id="root" alignment="CENTER_LEFT" prefHeight="500.0" prefWidth="600.0"
    xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ToolBar>
            <TextField fx:id="search" maxWidth="1.7976931348623157E308"
                promptText="Node ids (eg: 42 43 44)" HBox.hgrow="ALWAYS" />
            <Button fx:id="reset" mnemonicParsing="false" text="Reset viewport" />
        </ToolBar>
        <StackPane fx:id="mapContainer" VBox.vgrow="ALWAYS" />
        <ToolBar>
            <items>
                <Label fx:id="message" text="Label" />
            </items>
        </ToolBar>
    </children>
</VBox>
